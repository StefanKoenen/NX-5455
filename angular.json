{
	"version": 1,
	"projects": {
		"dev-app": {
			"projectType": "application",
			"schematics": {
				"@nrwl/angular:component": {
					"style": "scss"
				}
			},
			"root": "apps/dev-app",
			"sourceRoot": "apps/dev-app/src",
			"prefix": "tm",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"allowedCommonJsDependencies": [
							"devextreme-angular",
							"devextreme",
							"jwt-decode",
							"file-saver",
							"lodash"
						],
						"outputPath": "dist/apps/dev-app",
						"index": "apps/dev-app/src/index.html",
						"main": "apps/dev-app/src/main.ts",
						"polyfills": "apps/dev-app/src/polyfills.ts",
						"tsConfig": "apps/dev-app/tsconfig.app.json",
						"aot": true,
						"assets": [
							"apps/dev-app/src/favicon.ico",
							"apps/dev-app/src/assets/icons",
							"apps/dev-app/src/assets/media",
							"apps/dev-app/src/manifest.webmanifest"
						],
						"styles": [{
								"input": "apps/dev-app/src/devextreme.scss",
								"bundleName": "devextreme",
								"inject": true
							},
							{
								"input": "apps/dev-app/src/app.scss",
								"bundleName": "app",
								"inject": true
							},
							{
								"input": "apps/dev-app/src/assets/sass/pages/login/login-3.scss",
								"bundleName": "login",
								"inject": true
							},
							"apps/dev-app/src/styles.scss"
						]
					},
					"configurations": {
						"production": {
							"fileReplacements": [{
								"replace": "apps/dev-app/src/environments/environment.ts",
								"with": "apps/dev-app/src/environments/environment.prod.ts"
							}],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb"
								}
							],
							"serviceWorker": true,
							"ngswConfigPath": "apps/dev-app/ngsw-config.json"
						}
					},
					"outputs": [
						"{options.outputPath}"
					]
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "dev-app:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "dev-app:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "dev-app:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": [
							"apps/dev-app/tsconfig.app.json",
							"apps/dev-app/tsconfig.spec.json"
						],
						"exclude": [
							"**/node_modules/**",
							"!apps/dev-app/**"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"options": {
						"jestConfig": "apps/dev-app/jest.config.js",
						"passWithNoTests": true
					},
					"outputs": [
						"coverage/apps/dev-app"
					]
				}
			}
		},
		"dev-app-e2e": {
			"root": "apps/dev-app-e2e",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@angular-devkit/build-angular:protractor",
					"options": {
						"protractorConfig": "apps/dev-app-e2e/protractor.conf.js",
						"devServerTarget": "dev-app:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "dev-app:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"tsConfig": "apps/dev-app-e2e/tsconfig.e2e.json",
						"exclude": [
							"**/node_modules/**",
							"!apps/dev-app-e2e/**"
						]
					}
				}
			}
		}
	},
	"cli": {
		"defaultCollection": "@nrwl/angular",
		"analytics": false,
		"packageManager": "pnpm"
	},
	"schematics": {
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"publishable": true,
			"prefix": "tm",
			"routing": true,
			"lazy": true
		},
		"@nrwl/angular:component": {
			"style": "scss",
			"styleext": "scss"
		},
		"@schematics/angular:component": {
			"styleext": "scss",
			"style": "scss",
			"changeDetection": "OnPush"
		}
	},
	"defaultProject": "dev-app"
}
